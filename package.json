    "mobx-react-router": "^4.0.7",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-i18next": "^10.11.2",
    "react-loadable": "^5.5.0",
    "react-router": "^5.0.1",
